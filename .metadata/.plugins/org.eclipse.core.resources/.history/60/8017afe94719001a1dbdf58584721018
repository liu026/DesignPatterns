public class Jeweler {
    private Gem ruby;
    private Gem emerald;
    private Gem sapphire;
    
    public Jeweler() {
        ruby = new Ruby();
        emerald = new Emerald();
        sapphire = new Sapphire();
    }
    
    public void feedLion() {
        ruby.cut();
    }
    
    public void feedWolf() {
        emerald.cut();
    }
    
    public void feedBear() {
        sapphire.cut();
    }
}